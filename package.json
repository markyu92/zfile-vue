		"vue": "^3.3.4",
		"vue-request": "^2.0.4",
		"vue-router": "^4.2.2",
		"vue-tsc": "^1.6.5"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"rollup",
				"markdown-it",
				"@types/markdown-it"
			]
		}
	},
	"lint-staged": {
		"*.{js,jsx,vue}": "eslint --cache --fix"
	}
}
