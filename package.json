		"artplayer": "^5.0.9",
		"artplayer-plugin-danmuku": "^5.0.1",
		"async-validator": "^4.2.5",
		"beautify-qrcode": "^1.0.3",
		"codemirror": "^6.x",
		"dayjs": "^1.11.7",
		"detect-file-encoding-and-language": "^2.4.0",
		"element-plus": "^2.9.10",
		"file-saver": "^2.0.5",
		"github-markdown-css": "^5.2.0",
		"highlight.js": "^11.8.0",
		"hls.js": "^1.4.4",
		"iconv-lite": "^0.6.3",
		"lazysizes": "^5.3.2",
		"less": "^4.1.3",
		"marked": "^5.0.4",
		"md5": "^2.3.0",
		"minimatch": "^9.0.1",
		"mpegts.js": "^1.7.3",
		"path-browserify": "^1.0.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"sortablejs": "^1.15.0",
		"ua-browser": "^0.1.7",
		"v-contextmenu": "^3.0.0",
		"v3-img-preview-enhance": "^1.1.18",
		"vue-3d-loader": "^2.2.4",
		"vue-codemirror": "^6.1.1",
		"vue-pdf-embed": "^2.1.0",
		"xlsx": "^0.18.5"
	},
